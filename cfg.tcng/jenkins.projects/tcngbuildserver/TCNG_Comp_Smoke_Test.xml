    <hudson.tasks.Shell>
      <command>whoami
env
pwd

if [ -z &quot;$TCNG_FW&quot; ]; then
    export TCNG_FW=&quot;TCNG_Nightly_Jenkins_&quot;$( date +%F )
fi

if [ $TCNG_BUILD_TYPE == &quot;PRODUCTION&quot; ]
then
   	JENKINS_IMAGE_DIR=production_tcng_jenkins_images       
else
	JENKINS_IMAGE_DIR=latest_tcng_jenkins_images
fi
export TNT_PLATFORM_NAME=tcng_at91cap9_e132mb

if [ -e TCNG_BUILD_TARBALL.tar.gz ]
then
	echo &quot;Going to untar the tarball for testing ... TCNG_BUILD_TARBALL.tar.gz&quot;
    export FLASH_IMAGE_PATH=TCNG_BUILD_TARBALL.tar.gz
else
	echo &quot;No tarball provided, so use the default one ...&quot;
	export FLASH_IMAGE_PATH=`ls -t /tmp/buildhost/$JENKINS_IMAGE_DIR/$TCNG_FW/tcng_at91cap9_e132mb/*.bin | head -1`
fi

rm -rf var; rm -rf test_reports
tar --atime-preserve -zxvf /tmp/buildhost/$JENKINS_IMAGE_DIR/$TCNG_FW/tcng_package_qa/&quot;tcng_package_qa_&quot;$BUILD_NUMBER&quot;.tar.gz&quot; &gt; /dev/null

export TCNG_PACKAGE_ROOT=${WORKSPACE}/var/workspace/TCNG_Software
export TCNG_CFG_DIR=${HOME}/workspace/TCNG_QA/ci

echo &quot;Going to setup the environments for TCNG framework ...&quot;
source $TCNG_CFG_DIR/cfg.tcng/source_me_first_tcng_env.sh

echo &quot;Goging to set the testsuite: $TCNG_TESTSUITE ...&quot;
export TCNG_TEST_SUITE=$TNT_TCNG_QA_TOPDIR/testcases/Common/Component/TCNGComponentSmokeTest/TCNGComponentSmokeTest.xml

echo &quot;Going to flash the testbed and run the test suite ...&quot;
$TCNG_CFG_DIR/myex.pl --config $TCNG_CFG_DIR/cfg.tcng/TCNG_Test_Only.cfg

if [ -e test_reports ]
then
	echo &quot;Goging to save the testing logs&quot;
	test_report_tarball=TCNG_Comp_Smoke_TestResult.tar.gz
    tar -zcvf ${test_report_tarball} test_reports
    dest_dir=&quot;/tmp/buildhost/$JENKINS_IMAGE_DIR/$TCNG_FW/tcng_at91cap9_e132mb/test_reports&quot;
	echo &quot;Goging to save the testing logs under ${dest_dir} ...&quot;
    mkdir -p ${dest_dir} &amp;&amp; cp ${test_report_tarball} ${dest_dir}
fi

# please replace this with
# /workspace/jenkins_run_smoke_tests.sh</command>
    </hudson.tasks.Shell>
