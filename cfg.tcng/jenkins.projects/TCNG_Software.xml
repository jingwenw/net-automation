<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Build and test</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>10</daysToKeep>
        <numToKeep>10</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>PLATFORM_NAME</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>tcng_at91cap9_tc_1220_rd</string>
              <string>tcng_at91cap9_tc_1216</string>
              <string>tcng_at91cap9_tc_1210_rd</string>
              <string>tcng_at91cap9_pp_1316</string>
              <string>tcng_at91cap9_e132mb</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TCNG_FW</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>default</string>
              <string>TCNG_FW1143</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TCNG_BUILD_TYPE</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>latest</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TCNG_TESTSUITE</name>
          <description>Please select the test suite</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>ProductSmokeTest</string>
              <string>ProductCommonTest</string>
              <string>I210SmokeTest</string>
              <string>PP131XSmokeTest</string>
              <string>ProductRegression</string>
              <string>TCNGComponentSmokeTest</string>
              <string>TCNGComponentFullTest</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>UPSTREAM_BUILD</name>
          <description></description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.mercurial.MercurialSCM" plugin="mercurial@2.1">
    <source>ssh://hguser@hg//var/hg/TCNG_Software</source>
    <modules></modules>
    <revisionType>BRANCH</revisionType>
    <revision>default</revision>
    <clean>false</clean>
    <credentialsId></credentialsId>
    <disableChangeLog>false</disableChangeLog>
  </scm>
  <assignedNode>c1s</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec></spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>whoami
pwd


echo &quot; ******************* $TCNG_FW *****************************&quot;

echo &quot;Going to setup the environments for TCNG fram ...&quot;
export TCNG_CFG_DIR=${HOME}/myRepos/net-automation
export TNT_PLATFORM_NAME=$PLATFORM_NAME
export TCNG_TEST_SUITE=$TCNG_CFG_DIR/cfg.tcng/$TCNG_TESTSUITE.xml

echo &quot;Going to build and test for $TNT_PLATFORM_NAME...&quot;
${TCNG_CFG_DIR}/cfg.tcng/launch_tcng_build_and_test.sh $TCNG_TEST_SUITE

</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.junit.JUnitResultArchiver plugin="junit@1.21">
      <testResults>**/TC_*.xml</testResults>
      <keepLongStdio>true</keepLongStdio>
      <healthScaleFactor>1.0</healthScaleFactor>
      <allowEmptyResults>false</allowEmptyResults>
    </hudson.tasks.junit.JUnitResultArchiver>
  </publishers>
  <buildWrappers/>
</project>